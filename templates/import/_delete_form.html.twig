<form method="post" action="{{ path('import_delete', {'id': import.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimer le fichier ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ import.id) }}">
    <button class="user-delete"><i class="fas fa-trash-alt"></i></button>
</form>
